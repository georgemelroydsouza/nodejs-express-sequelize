  "dependencies": {
    "express": "^4.17.1",
    "joi": "^17.3.0",
    "mongoose": "^5.11.5",
    "mysql2": "^2.2.5",
    "nodemon": "^2.0.6",
    "sequelize": "^6.3.5"
  },
  "devDependencies": {
    "chai": "^4.2.0",
